	Issn = {1911-4745(ELECTRONIC)},
	Journal = {Comparative Cognition \& Behavior Reviews},
	Keywords = {review,concept formation,concepts,animal learning},
	Pages = {13-45},
	Title = {Concept Learning in Animals},
	Volume = {3},
	Year = {2008},
	Bdsk-Url-1 = {https://doi.org/10.3819/ccbr.2008.30002}}


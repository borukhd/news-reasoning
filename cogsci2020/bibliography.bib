	Language = {en},
	Month = dec,
	Number = {4},
	Pages = {282-306},
	Shorttitle = {Relational {{Spatial Reasoning}} by a {{Nonhuman}}},
	Title = {Relational {{Spatial Reasoning}} by a {{Nonhuman}}: {{The Example}} of {{Capuchin Monkeys}}},

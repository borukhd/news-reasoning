	Date-Modified = {2020-01-23 15:39:02 +0100},
	Doi = {10.1037/a0025764},
	File = {/home/paulina/Zotero/storage/ALCDAF78/Green et al_2012_Neural correlates of creativity in analogical reasoning.pdf},
	Issn = {1939-1285, 0278-7393},
	Journal = {Journal of Experimental Psychology: Learning, Memory, and Cognition},
	Language = {en},

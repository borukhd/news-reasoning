	Bdsk-Url-1 = {https://doi.org/10.1901/jeab.1993.59-265}}

@article{bunsey1993,
	Abstract = {Human amnesics are severely impaired in learning arbitrary associations between word pairs. This report examines the role of the hippocampal system in an analog of paired-associate learning developed for rats. Ss with selective damage to the parahippocampal region and controls were tested on an odor-guided paired-associate task. Normal rats readily learned to distinguish paired associates from various mispairings of the same odors, whereas most animals with parahippocampal damage failed to learn. Lesioned animals were, however, able to acquire correct responses to stimulus pairs for which an association between odors was not required. These findings extend the usefulness of olfactory learning as a model for studies of higher order learning capacity in rodents and indicate that in rats, as in humans, hippocampal areas are critical for learning arbitrary nonspatial relationships between stimuli. (PsycINFO Database Record (c) 2016 APA, all rights reserved)},

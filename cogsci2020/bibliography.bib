	File = {/home/paulina/Zotero/storage/EJZMJFPL/Viskontas et al_2005_Relational integration in older adults.pdf;/home/paulina/Zotero/storage/KQHFUXAD/Viskontas et al_2005_Relational integration in older adults.pdf},
	Issn = {13546783},
	Journal = {Thinking \& Reasoning},
	Keywords = {memory,reasoning (logic),short-term memory,reason,thinking},
	Month = nov,
	Number = {4},
	Pages = {390-410},
	Title = {Relational Integration in Older Adults},

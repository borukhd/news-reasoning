	Issn = {1089-778X},
	Journal = {IEEE Transactions on Evolutionary Computation},
	Keywords = {dynamic adaptation,dynamic cellular genetic algorithms,exploitation tradeoff,exploration tradeoff,genetic algorithms,neighborhood-to-population ratio,search model,search problems,search selection pressure,stochastic processes,cellular genetic algorithm (cga),computer science,constraint optimization,evolutionary algorithm (ea),evolutionary computation,partitioning algorithms,shape,space exploration,topology},
	Month = apr,

	Issn = {1354-6783},
	Journal = {Thinking \& Reasoning},
	Keywords = {psychology,reasoning,experimental design,heuristic,theory of knowledge},
	Month = may,
	Number = {2},
	Pages = {164-187},
	Shorttitle = {The Relevance of Selecting What's Relevant},
	Title = {The Relevance of Selecting What's Relevant: {{A}} Dual Process Approach to Transitive Reasoning with Spatial Relations},
	Volume = {13},
	Year = {2007},
	Bdsk-Url-1 = {https://doi.org/10.1080/13546780600780671}}

@article{halford2002,
	Abstract = {Three experiments investigated the effect of complexity on children's understanding of a beam balance. In nonconflict problems, weights or distances varied, while the other was held constant. In conflict items, both weight and distance varied, and items were of three kinds: weight dominant, distance dominant, or balance (in which neither was dominant). In Experiment 1, 2-year-old children succeeded on nonconflict-weight and nonconflict-distance problems. This result was replicated in Experiment 2, but performance on conflict items did not exceed chance. In Experiment 3, 3- and 4-year-olds succeeded on all except conflict balance problems, while 5- and 6-year-olds succeeded on all problem types. The results were interpreted in terms of relational complexity theory. Children aged 2 to 4 years succeeded on problems that entailed binary relations, but 5- and 6-year-olds also succeeded on problems that entailed ternary relations. Ternary relations tasks from other domains\textemdash{}transitivity and class inclusion\textemdash{}accounted for 93\% of the age-related variance in balance scale scores.},
	Author = {Halford, Graeme S and Andrews, Glenda and Dalton, Cherie and Boag, Christine and Zielinski, Tracey},

	File = {/home/paulina/Zotero/storage/XRLBDRRH/O'Reilly_Rudy_2000_Computational principles of learning in the neocortex and hippocampus.pdf},
	Issn = {1050-9631, 1098-1063},
	Journal = {Hippocampus},
	Language = {en},
	Number = {4},
	Pages = {389-397},
	Title = {Computational Principles of Learning in the Neocortex and Hippocampus},

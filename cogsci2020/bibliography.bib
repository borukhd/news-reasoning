	Issn = {1532-5946},
	Journal = {Memory \& Cognition},
	Keywords = {transitivity,categorization,causal induction,causal learning,causality,knowledge-based induction,markov condition,mixing of causal relationships,probabilistic reasoning,transitive distortion effects},
	Language = {en},
	Month = apr,
	Number = {3},

	Month = may,
	Number = {3},
	Pages = {516-529},
	Shorttitle = {A Computational Account of Children's Analogical Reasoning},
	Title = {A Computational Account of Children's Analogical Reasoning: Balancing Inhibitory Control in Working Memory and Relational Representation: {{Development}} of Analogical Reasoning},
	Volume = {14},

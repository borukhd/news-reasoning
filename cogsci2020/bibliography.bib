@article{rogers2010,
	Abstract = {The default network is a set of brain regions that exhibit a reduction in BOLD response during attention-demanding cognitive tasks, and distinctive patterns of functional connectivity that typically include anti-correlations with a fronto-parietal network involved in attention, working memory, and executive control. The function of the default network regions has been attributed to introspection, self-awareness, and theory of mind judgments, and some of its regions are involved in episodic memory processes.},
	Author = {Rogers, Baxter P. and Avery, Suzanne N. and Heckers, Stephan},
	Date-Added = {2020-01-23 15:39:02 +0100},

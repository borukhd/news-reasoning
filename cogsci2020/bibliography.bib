@article{mascarenhas2017,
	Abstract = {The psychological study of reasoning with quantifiers has predominantly focused on inference patterns studied by Aristotle about two millennia ago. Modern logic has shown a wealth of inference patterns involving quantifiers that are far beyond the expressive power of Aristotelian syllogisms, and whose psychology should be explored. We bring to light a novel class of fallacious inference patterns, some of which are so attractive that they are tantamount to cognitive illusions. In tandem with recent insights from linguistics that quantifiers like ``some'' are treated aswh-questions, these illusory inferences are predicted by the erotetic theory of reasoning, which postulates that a process akin to question asking and answering is behind human inference making.},
	Author = {Mascarenhas, Salvador and Koralus, Philipp},
	Date-Added = {2020-01-23 15:39:02 +0100},
	Date-Modified = {2020-01-23 15:39:02 +0100},
	Doi = {10.1080/13546783.2016.1167125},
	File = {/home/paulina/Zotero/storage/VYYT37N4/Mascarenhas_Koralus_2017_Illusory inferences with quantifiers.pdf},
	Issn = {13546783},
	Journal = {Thinking \& Reasoning},
	Keywords = {reasoning,mental models,quantifiers,questions,syllogisms,hallucinations \& illusions},
	Month = feb,
	Number = {1},
	Pages = {33-48},
	Title = {Illusory Inferences with Quantifiers},
	Volume = {23},

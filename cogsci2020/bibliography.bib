	File = {/home/paulina/Zotero/storage/H2CEZVUZ/Wendelken_Bunge_2009_Transitive Inference.pdf;/home/paulina/Zotero/storage/5GX62XDI/jocn.2009.html},
	Issn = {0898-929X},
	Journal = {Journal of Cognitive Neuroscience},
	Month = mar,
	Number = {5},
	Pages = {837-847},
	Shorttitle = {Transitive {{Inference}}},
	Title = {Transitive {{Inference}}: {{Distinct Contributions}} of {{Rostrolateral Prefrontal Cortex}} and the {{Hippocampus}}},
	Volume = {22},
	Year = {2009},

	Issn = {0093-934X},
	Journal = {Brain and language},
	Month = may,
	Number = {2},
	Pages = {59-64},
	Pmcid = {PMC2994599},
	Pmid = {20206985},
	Shorttitle = {Is {{Relational Reasoning Dependent}} on {{Language}}?},
	Title = {Is {{Relational Reasoning Dependent}} on {{Language}}? {{A Voxel}}-{{Based Lesion Symptom Mapping Study}}},
	Volume = {113},

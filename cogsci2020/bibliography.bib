	Issn = {1531-5320},
	Journal = {Psychonomic Bulletin \& Review},
	Keywords = {relational reasoning,bayesian inference,natural frequencies,numeracy,question form,structural mapping},
	Language = {en},
	Month = jun,
	Number = {3},
	Pages = {964-971},
	Shorttitle = {Structural Mapping in Statistical Word Problems},
	Title = {Structural Mapping in Statistical Word Problems: {{A}} Relational Reasoning Approach to {{Bayesian}} Inference},

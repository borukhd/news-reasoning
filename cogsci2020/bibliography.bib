	Doi = {10.1002/hipo.10046},
	File = {/home/paulina/Zotero/storage/A2LTV627/Ohta et al_2002_Deterioration in learning and memory of inferential tasks for evaluation of.pdf},
	Issn = {1050-9631, 1098-1063},
	Journal = {Hippocampus},
	Keywords = {age differences,mice,modelme},
	Language = {en},
	Number = {6},
	Pages = {803-810},
	Title = {Deterioration in Learning and Memory of Inferential Tasks for Evaluation of Transitivity and Symmetry in Aged {{SAMP8}} Mice},
	Volume = {12},
	Year = {2002},

	File = {/home/paulina/Zotero/storage/ZBPAY2MJ/Bouwmeester et al_2007_Development and individual differences in transitive reasoning.pdf;/home/paulina/Zotero/storage/FXJRN9LQ/S0273229706000621.html},
	Issn = {0273-2297},
	Journal = {Developmental Review},
	Keywords = {cognitive modeling,transitive reasoning,cognitive development,individual differences,fuzzy trace theory,multilevel latent class analysis},
	Month = mar,
	Number = {1},
	Pages = {41-74},
	Shorttitle = {Development and Individual Differences in Transitive Reasoning},
	Title = {Development and Individual Differences in Transitive Reasoning: {{A}} Fuzzy Trace Theory Approach},

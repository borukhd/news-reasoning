	File = {/home/paulina/Zotero/storage/N55CCSKD/Moses et al_2008_Cognitive integrity predicts transitive inference performance bias and success.pdf;/home/paulina/Zotero/storage/5C8HT8K7/S0028393207004411.html},
	Issn = {0028-3932},
	Journal = {Neuropsychologia},
	Keywords = {transitive inference,memory,learning,relations},

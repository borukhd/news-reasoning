	Title = {Transitivity and the Representation of Stimulus Relations by Young Children},
	Type = {Ph.{{D}}.},
	Year = {1978}}

@article{kingma1984,
	Abstract = {300 Ss from kindergarten and Grade 1 in the Netherlands were divided into 4 age levels and administered 3 types of seriation tasks (length seriation, seriation tasks provided with salience cues, and number seriation tasks). Also, ordinal correspondence and cardination tasks were completed. Findings show that the relationship between seriation, ordinal correspondence, and cardination (as well as their sequence of development) was influenced by varying task sensitivity (i.e., encoding load of the tasks). It is suggested that with respect to the predicted sequences of development, any theory can be confirmed or disconfirmed by manipulating perceptual (encoding load) features of the tasks. Lowering the encoding load of the seriation task, by adding salience cues, produced a facilitative effect on the seriation performance. (67 ref) (PsycINFO Database Record (c) 2016 APA, all rights reserved)},

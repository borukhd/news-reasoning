@techreport{munoz2019,
	Abstract = {Monkeys are able to learn the implied ordering of pairs of images drawn from an ordered set, without ever seeing all of the images simultaneously, and without explicit spatial or temporal cues. Learning is evidenced by their ability to make transitive inferences (TI), which are judgments of serial order based on the transitive relation that A{$>$}B and B{$>$}C implies A{$>$}C. Because posterior parietal cortex has been shown to integrate spatial and numerical information, we recorded the activity of parietal neurons in rhesus macaques while they learned 5- and 7-item TI lists. Symbolic distance (but not ordinal position) strongly predicted decision accuracy, and learning was consistently faster as symbolic distance increased. Neurons exhibited selectivity for stimulus identity during learning, but not before or after. Furthermore, across the population of neurons, there was significant modulation of firing correlated with the relative rank of the two stimuli presented on each trial. The observed behavior during learning is best explained by a virtual workspace model, not by associative or reward mechanisms. The neural data support a role for posterior parietal cortex in representing several variables that contribute to serial learning, particularly information about the ordinal ranks of both stimuli presented during a given trial. Thus, parietal cortex can be considered as part of the neural substrate for learning and representing abstract relationships in a virtual workspace.},
	Author = {Munoz, Fabian and Jensen, Greg and Kennedy, Benjamin C. and Alkan, Yelda and Terrace, Herbert S. and Ferrera, Vincent P.},
	Date-Added = {2020-01-23 15:39:02 +0100},
	Date-Modified = {2020-01-23 15:39:02 +0100},
	Doi = {10.1101/689133},
	File = {/home/paulina/Zotero/storage/RN5NIDPF/Munoz et al. - 2019 - Neuronal Responses in Posterior Parietal Cortex du.pdf},
	Institution = {{Neuroscience}},
	Language = {en},
	Month = jul,
	Title = {Neuronal {{Responses}} in {{Posterior Parietal Cortex}} during {{Learning}} of {{Implied Serial Order}}},
	Type = {Preprint},
	Year = {2019},
	Bdsk-Url-1 = {https://doi.org/10.1101/689133}}

@article{jensen2019,
	Abstract = {Humans and animals can learn to order a list of items without relying on explicit spatial or temporal cues. To do so, they appear to make use of transitivity, a property of all ordered sets. Here, we summarize relevant research on the transitive inference (TI) paradigm and its relationship to learning the underlying order of an arbitrary set of items. We compare six computational models of TI performance, three of which are model-free (Q-learning, Value Transfer, and REMERGE) and three of which are model-based (RL-Elo, Sequential Monte Carlo, and Betasort). Our goal is to assess the ability of these models to produce empirically observed features of TI behavior. Model-based approaches perform better under a wider range of scenarios, but no single model explains the full scope of behaviors reported in the TI literature.},
	Author = {Jensen, Greg and Terrace, Herbert S. and Ferrera, Vincent P.},
	Doi = {10.3389/fnins.2019.00878},
	File = {/home/paulina/Zotero/storage/NIXB329V/Jensen et al. - 2019 - Discovering Implied Serial Order Through Model-Fre.pdf},
	Issn = {1662-453X},
	Journal = {Frontiers in Neuroscience},
	Keywords = {transitive inference,Cognitive Maps,model-based learning,model-free learning,reinforcement learning},
	Language = {English},
	Title = {Discovering {{Implied Serial Order Through Model}}-{{Free}} and {{Model}}-{{Based Learning}}},
	Volume = {13},
	Year = {2019},
	Bdsk-Url-1 = {https://doi.org/10.3389/fnins.2019.00878}}


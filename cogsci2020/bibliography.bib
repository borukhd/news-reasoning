	Issn = {0896-6273},
	Journal = {Neuron},
	Keywords = {Inference,Cognitive Map,Decision Making,Generalization,Hippocampal Formation,Prefrontal Cortex,Reinforcement Learning,Spatial Cognition,Statistical Learning,Structure Learning},
	Language = {en},
	Month = oct,
	Number = {2},
	Pages = {490-509},

	Journal = {Behavioural Brain Research},
	Keywords = {stimulus equivalence,categories,concepts,directly trained,event-related potentials,relatedness,relational inference},
	Month = jan,
	Pages = {332-343},
	Title = {Event-Related Potential Correlates of Emergent Inference in Human Arbitrary Relational Learning},

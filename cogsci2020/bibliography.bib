	Keywords = {problem solving,memory,hippocampus,learning,rats,artificial neural network,animals,discrimination learning,models,brain mapping,association learning,cerebral cortex,conditioning,fear,incidental learning,interference (learning),retention (psychology),human},
	Number = {2},

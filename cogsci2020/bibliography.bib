	Bdsk-Url-1 = {https://doi.org/10.1002/hbm.21336}}

@article{smith1986,
	Abstract = {A distinctive feature of \emph{Piaget's} genetic epistemology is illustrated and discussed with reference to \emph{La Gen{\`e}se du Number/The Child's Conception of Number}. Genetic epistemology is taken to require the joint use both of philosophical analysis, which serves to identify the logical ordering of concepts, and of empirical methods, which serve to establish their temporal ordering in children's understanding. The method of critical exploration is an empirical, even if not an experimental, method. It is used to establish not which external variables influence a child's performance but whether the conditions extracted from a philosophical analysis have been satisfied. The central argument is assessed by its capacity to withstand recent philosophical and psychological criticisms. \emph{Piaget's} genetic epistemology requires a joint and complementary interest in philosophical and empirical issues.},

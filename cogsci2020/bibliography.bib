	Issn = {0960-9822},
	Journal = {Current Biology},
	Month = mar,
	Number = {7},
	Pages = {821-830},
	Title = {Insight {{Reconfigures Hippocampal}}-{{Prefrontal Memories}}},
	Volume = {25},

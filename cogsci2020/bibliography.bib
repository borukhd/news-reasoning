	Keywords = {deduction,hemispheric specialization,reasoning,transitive inference},
	Language = {English},
	Shorttitle = {Hemispheric {{Differences}} in {{Relational Reasoning}}},
	Title = {Hemispheric {{Differences}} in {{Relational Reasoning}}: {{Novel Insights Based}} on an {{Old Technique}}},

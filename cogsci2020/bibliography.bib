	Issn = {1939-2184(ELECTRONIC),0097-7403(PRINT)},
	Journal = {Journal of Experimental Psychology: Animal Behavior Processes},
	Keywords = {serial position effect,list linking,distance perception,macaques,modelme,associative value,spatial representation},

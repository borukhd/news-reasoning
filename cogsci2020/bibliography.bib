	File = {/home/paulina/Zotero/storage/LL7DCVTS/Daniels et al_2014_Transitive inference by pigeons.pdf},
	Issn = {1435-9456},
	Journal = {Animal Cognition},
	Keywords = {transitive inference,5-term series,linear ordering,successive-pair training},
	Language = {en},
	Number = {4},
	Pages = {973-981},
	Shorttitle = {Transitive Inference by Pigeons},
	Title = {Transitive Inference by Pigeons: {{Does}} the Geometric Presentation of the Stimuli Make a Difference?},
	Volume = {17},
	Year = {2014},
	Bdsk-Url-1 = {https://doi.org/10.1007/s10071-014-0729-0}}


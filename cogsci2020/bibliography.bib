	Issn = {1938-3711},
	Journal = {Journal of the Experimental Analysis of Behavior},
	Keywords = {transitive inference,5-term series,pigeon,n-term series problems,modelme},
	Language = {en},
	Number = {2},
	Pages = {265-291},
	Title = {``{{Transitive Inference}}'' in {{Multiple Conditional Discriminations}}},
	Volume = {59},

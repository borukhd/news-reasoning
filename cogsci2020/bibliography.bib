	File = {/home/paulina/Zotero/storage/6N2AAE29/Wendelken et al_2012_Rostrolateral prefrontal cortex.pdf},
	Issn = {10659471},
	Journal = {Human Brain Mapping},
	Language = {en},
	Month = aug,
	Number = {8},
	Pages = {1952-1963},
	Shorttitle = {Rostrolateral Prefrontal Cortex},
	Title = {Rostrolateral Prefrontal Cortex: {{Domain}}-General or Domain-Sensitive?},
	Volume = {33},

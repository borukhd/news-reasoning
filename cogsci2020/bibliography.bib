	Journal = {Cerebral Cortex},
	Number = {5},
	Pages = {2178-2190},
	Title = {Fronto-{{Parietal Network Reconfiguration Supports}} the {{Development}} of {{Reasoning Ability}}},
	Volume = {26},
	Year = {2016}}


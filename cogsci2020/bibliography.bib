	File = {/home/paulina/Zotero/storage/Q97S3PS4/Ryan et al_2009_Impaired relational organization of propositions, but intact transitive.pdf;/home/paulina/Zotero/storage/7X69VE2G/S0028393208003710.html},
	Issn = {0028-3932},
	Journal = {Neuropsychologia},
	Keywords = {transitive inference,memory,hippocampus,relational memory},
	Number = {2},
	Pages = {338-353},
	Shorttitle = {Impaired Relational Organization of Propositions, but Intact Transitive Inference, in Aging},
	Title = {Impaired Relational Organization of Propositions, but Intact Transitive Inference, in Aging: {{Implications}} for Understanding Underlying Neural Integrity},
	Volume = {47},

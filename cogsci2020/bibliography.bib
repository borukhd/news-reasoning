	Issn = {0735-7036},
	Journal = {Journal of Comparative Psychology},
	Keywords = {animals,human,modelme},
	Number = {3},
	Pages = {231-245},
	Pmcid = {PMC5552434},
	Pmid = {28333486},
	Title = {Transitive Inference in Humans and Rhesus Macaques after Massed Training of the Last Two List Items},
	Volume = {131},

	Keywords = {relational reasoning,analogy,anomaly,antinomy,antithesis},
	Language = {en},
	Month = sep,
	Number = {3},
	Pages = {391-427},
	Shorttitle = {Relational {{Reasoning}} and {{Its Manifestations}} in the {{Educational Context}}},
	Title = {Relational {{Reasoning}} and {{Its Manifestations}} in the {{Educational Context}}: A {{Systematic Review}} of the {{Literature}}},
	Volume = {25},

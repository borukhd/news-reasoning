	File = {/home/paulina/Zotero/storage/236BCV3N/Bouwmeester_Sijtsma_2007_Latent Class Modeling of Phases in the Development of Transitive Reasoning.pdf;/home/paulina/Zotero/storage/9GUXPBFF/Bouwmeester_Sijtsma_2007_Latent class modeling of phases in the development of transitive reasoning.pdf;/home/paulina/Zotero/storage/9JG37FN9/Bouwmeester_Sijtsma_2007_Latent Class Modeling of Phases in the Development of Transitive Reasoning.pdf},
	Issn = {0027-3171},
	Journal = {Multivariate Behavioral Research},
	Keywords = {reasoning,fuzzy measure theory,developmental},

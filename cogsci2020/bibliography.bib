	File = {/home/paulina/Zotero/storage/HRQ4YVRW/Tanner et al_2017_Inferential Learning of Serial Order of Perceptual Categories by Rhesus Monkeys.pdf;/home/paulina/Zotero/storage/U4V9GQKL/6268.html},
	Issn = {0270-6474, 1529-2401},
	Journal = {Journal of Neuroscience},
	Keywords = {transitive inference,categorization,serial learning},
	Language = {en},

	Issn = {00207594},
	Journal = {International Journal of Psychology},
	Keywords = {cognitive development,child development,developmental},
	Month = oct,
	Number = {5},
	Pages = {258-273},
	Title = {A Century of {{Chinese}} Developmental Psychology},
	Volume = {38},

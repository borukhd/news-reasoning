	File = {/home/paulina/Zotero/storage/3MKSKZG9/Ihmels et al_2016_Relational integrativity of prime-target pairs moderates congruity effects in.pdf},
	Issn = {0090502X},
	Journal = {Memory \& Cognition},
	Keywords = {relational integration,affective priming,confounding variables,evaluative priming,integrativity,materials testing,mathematics,paired comparisons (mathematics),phonological awareness,semantic priming,semantics},
	Month = may,
	Number = {4},
	Pages = {565-579},
	Title = {Relational Integrativity of Prime-Target Pairs Moderates Congruity Effects in Evaluative Priming},
	Volume = {44},
	Year = {2016},
	Bdsk-Url-1 = {https://doi.org/10.3758/s13421-015-0581-8}}

@article{kovacs2016,
	Abstract = {The most replicated result in the field of intelligence is the positive manifold, which refers to an all-positive pattern of correlations among diverse cognitive tests. The positive manifold is typically described by a general factor, org. In turn,gis often identified as general intelligence, yet this explanation is contradicted by a number of results. Here we offer a new account ofg: process overlap theory. According to the theory, cognitive tests tap domain-general executive processes, identified primarily in research on working memory, as well as more domain-specific processes. Executive processes are tapped in an overlapping manner across cognitive tests such that they are required more often than domain-specific ones. The theory provides an account of a number of findings on human intelligence. As well, it is formalized as a multidimensional item response model and as a structural model, and the neural mechanisms underlying the proposed overlapping processes are discussed.},
	Author = {Kovacs, Kristof and Conway, Andrew R. A.},

	Doi = {10.1111/desc.12835},
	File = {/home/paulina/Zotero/storage/CG6CAMG8/Frankenhuis et al. - 2019 - Hidden talents in harsh conditions A preregistere.pdf;/home/paulina/Zotero/storage/H2Y6CWFV/Frankenhuis et al_2019_Hidden talents in harsh conditions.pdf},
	Issn = {1363-755X, 1467-7687},
	Journal = {Developmental Science},
	Language = {en},
	Month = may,
	Pages = {e12835},
	Shorttitle = {Hidden Talents in Harsh Conditions?},
	Title = {Hidden Talents in Harsh Conditions? {{A}} Preregistered Study of Memory and Reasoning about Social Dominance},

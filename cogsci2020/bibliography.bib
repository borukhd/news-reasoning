	Keywords = {transitive inference,hippocampus,schizophrenia,relational memory,bipolar disorder},
	Language = {en},
	Month = apr,
	Number = {3},
	Pages = {199-211},
	Title = {Relational Memory and Hippocampal Function in Psychotic Bipolar Disorder},
	Volume = {264},
	Year = {2014},

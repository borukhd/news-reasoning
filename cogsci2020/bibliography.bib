	Journal = {Animal Cognition},
	Keywords = {wild card sequences,chimpanzees,n-term series problems},
	Language = {en},
	Month = may,
	Number = {1},
	Pages = {1-11},
	Title = {Sequential Responding to Arabic Numerals with Wild Cards by the Chimpanzee ( {{Pan}} Troglodytes )},

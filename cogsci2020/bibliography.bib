	File = {/home/paulina/Zotero/storage/K7PZZH5Q/Prado et al_2010_Recomposing a fragmented literature.pdf;/home/paulina/Zotero/storage/CR4QMMX4/S1053811910003137.html},
	Issn = {1053-8119},
	Journal = {NeuroImage},
	Month = jul,
	Number = {3},
	Pages = {1213-1221},
	Shorttitle = {Recomposing a Fragmented Literature},
	Title = {Recomposing a Fragmented Literature: {{How}} Conditional and Relational Arguments Engage Different Neural Systems for Deductive Reasoning},

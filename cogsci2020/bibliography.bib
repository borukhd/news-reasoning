	Doi = {10.1016/0022-0965(81)90019-9},
	Journal = {Journal of Experimental Child Psychology},
	Month = apr,
	Pages = {300-309},
	Title = {Children's Memory for Premises in a Transitive Measurement Task Assessed by Elicited and Spontaneous Justifications},

	Issn = {1932-6203},
	Journal = {PLOS ONE},
	Keywords = {reasoning,working memory,meta-analysis,genetic loci,genome-wide association studies,heredity,twins},
	Language = {en},
	Month = apr,
	Number = {4},
	Pages = {e0123886},
	Title = {Genetic {{Basis}} of a {{Cognitive Complexity Metric}}},
	Volume = {10},
	Year = {2015},

	Pages = {616-627},
	Shorttitle = {Capacity {{Limitations}} in {{Children}}'s {{Reasoning}}},
	Title = {Capacity {{Limitations}} in {{Children}}'s {{Reasoning}}: {{A Dual}}-{{Task Approach}}},
	Volume = {57},

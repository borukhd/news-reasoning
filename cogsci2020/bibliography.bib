	File = {/home/paulina/Zotero/storage/6PIQAAP5/Anderson et al_1998_ACT-R.pdf},
	Issn = {0140-525X, 1469-1825},
	Journal = {Behavioral and Brain Sciences},
	Language = {en},
	Month = dec,
	Number = {6},
	Pages = {831-832},
	Shorttitle = {{{ACT}}-{{R}}},
	Title = {{{ACT}}-{{R}}: {{A}} Higher-Level Account of Processing Capacity},
	Volume = {21},
	Year = {1998},
	Bdsk-Url-1 = {https://doi.org/10.1017/S0140525X98221765}}

@article{riley1976,
	Abstract = {Two experiments tested a conjecture that a common representation, a linear order, underlies the processing of all comparatives. In Expt I, third-grade children learned the relations between adjacent pairs in a series of six faces and were tested on all possible pairs, using comparisons of height, weight, happiness, and niceness. Both the serial position effect in training and an inverse relationship between decision time and number of inferential steps in testing supported the hypothesis that linear orders were constructed from the pairwise relations. When allowed to organize a second set of faces as an external mnemonic, children constructed linear orders and training was dramatically facilitated. In Expt II, children who were shown nonlinear displays of pairs constructed linear memory representations in a task where memory was not required. The data strongly suggest that (a) linear orders are a common mode of representation for comparative relations, (b) people use these ordered sets to draw inferences about members of the set, and (c) formal deductive logic may not characterize the processes used by children to draw transitive inferences.},
	Author = {Riley, Christine A},
	Date-Added = {2020-01-23 15:39:02 +0100},

	Issn = {1932-6203},
	Journal = {PLOS ONE},
	Keywords = {reasoning,learning,children,behavior,cognitive psychology,statistical models,statistical theories},
	Language = {en},
	Month = oct,
	Number = {10},
	Pages = {e0136449},
	Shorttitle = {The {{Balance}}-{{Scale Task Revisited}}},
	Title = {The {{Balance}}-{{Scale Task Revisited}}: {{A Comparison}} of {{Statistical Models}} for {{Rule}}-{{Based}} and {{Information}}-{{Integration Theories}} of {{Proportional Reasoning}}},
